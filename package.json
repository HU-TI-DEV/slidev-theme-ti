{
    "name": "slidev-theme-ti",
    "version": "0.25.0",
    "description": "HBO-ICT TI theme for Slidev",
    "license": "MIT",
    "funding": "https://github.com/sponsors/antfu",
    "homepage": "https://sli.dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/slidevjs/themes"
    },
    "bugs": "https://github.com/slidevjs/themes/issues",
    "keywords": [
        "slidev-theme",
        "slidev"
    ],
    "engines": {
        "node": ">=14.0.0",
        "slidev": ">=v0.47.0"
    },
    "slidev": {
        "colorSchema": "dark",
        "defaults": {
            "fonts": {
                "sans": "Avenir",
                "serif": "Avenir",
                "mono": "Iosevka Custom Web"
            },
            "lineNumbers": true,
            "themeConfig": {
                "paginationX": "r",
                "paginationY": "b"
            },
            "aspectRatio": "16/9",
            "canvasWidth": 980
        }
    },
    "dependencies": {
        "@slidev/types": "^0.47.0"
    }
}
